      <xs:element name="match_child" type="matchchild"/>
      <xs:element name="match_content" type="matchcontent"/>
      <xs:element name="match_context" type="matchcontext"/>
      <xs:element name="match_distance" type="matchdistance"/>
      <xs:element name="match_hull" type="matchhull"/>
      <xs:element name="match_max_hull" type="matchmaxhull"/>
      <xs:element name="match_shield" type="matchshield"/>
      <xs:element name="match_size" type="matchsize"/>
      <xs:element name="match_parent" type="matchparent"/>
      <xs:element name="match_is_in_view_of" type="matchview"/>
      <xs:element name="match_infopoint_location" type="matchinfopointlocation" />
      <xs:element name="match_tradeoffer_location" type="matchtradeofferlocation" />
      <xs:element name="match_can_host_trade_offer" type="matchcanhosttradeoffer" />
      <xs:element name="match_buyoffer" type="matchbuyoffer" />
      <xs:element name="match_selloffer" type="matchselloffer" />
      <xs:element name="match_products" type="matchresources" />
      <xs:element name="match_resources" type="matchproducts" />
      <xs:element name="match_relation" type="matchrelation"/>
      <xs:element name="match_pilot" type="matchpilot" />
      <xs:element name="match_dock" type="matchdock"/>
      <xs:element name="match_summarised_adsign" type="matchsummarisedcontent">
        <xs:annotation>
          <xs:documentation>
            Match filter for summarised Ad Signs on a Defensible object
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="match_summarised_efficiencyupgrade" type="matchsummarisedcontent">
        <xs:annotation>
          <xs:documentation>
            Match filter for summarised Efficiency Upgrades on a Defensible object
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="match_summarised_flakcontroller" type="matchsummarisedcontent">
        <xs:annotation>
          <xs:documentation>
            Match filter for summarised Flak Controllers on a Defensible object
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="match_summarised_shield" type="matchsummarisedcontent">
        <xs:annotation>
          <xs:documentation>
            Match filter for summarised Shields on a Defensible object
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="match_summarised_surfacedestructible" type="matchsummarisedcontent">
        <xs:annotation>
